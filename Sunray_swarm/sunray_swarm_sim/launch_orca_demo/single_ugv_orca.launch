<launch>
    <node pkg="sunray_swarm" name="orca_node_ugv" type="orca_node" output="screen">
        <param name="agent_type" type="int" value="1" />
        <param name="agent_num" value="1" />
        <param name="agent_height" type="double" value="0.1" />	
        <param name="orca_params/neighborDist" value="1.0" />
        <param name="orca_params/timeHorizonObst" value="1.0" />
        <param name="orca_params/radius" value="0.3" />
        <param name="orca_params/maxSpeed" value="0.3" />
    </node>
</launch>

